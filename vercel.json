{
  "public": "public",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.php"
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.(css|js|jpg|jpeg|png|gif|ico|svg))",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000" }
      ]
    }
  ]
}
